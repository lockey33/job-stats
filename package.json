{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "prisma generate",
    "dev": "next dev",
    "dev:docker": "pnpm db:up && pnpm prisma:generate && pnpm db:wait && pnpm db:push && next dev",
    "prebuild": "prisma generate",
    "build": "next build",
    "build:vercel": "pnpm prisma:generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build:ci": "pnpm typecheck && pnpm lint && pnpm build",
    "dev:ensure-db": "node tools/dev/ensure-db.mjs",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d db",
    "db:wait": "node tools/dev/wait-for-db.mjs",
    "db:seed": "node scripts/dev-seed.mjs",
    "sync": "node scripts/sync.mjs",
    "sync:daily": "node scripts/sync.mjs --days=4",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.87.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "geist": "^1.5.1",
    "next": "15.5.2",
    "nuqs": "^2.6.0",
    "react": "19.1.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.18.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-simple-import-sort": "^12",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@10.0.0"
}
